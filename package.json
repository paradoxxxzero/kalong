{
  "name": "kalong",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --fix",
    "shell": "poetry run python -m kalong",
    "start": "concurrently --kill-others \"yarn dev\" \"wait-port 59998 && uv run kalong --front-port=59998 --detached ${FILE:-test.py}\"",
    "pypublish": "yarn build && poetry publish --build"
  },
  "license": "GPL-3.0-or-later",
  "private": true,
  "dependencies": {
    "@codemirror/autocomplete": "^6.19.0",
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/legacy-modes": "^6.5.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@lezer/highlight": "^1.1.3",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@uiw/react-codemirror": "^4.25.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "react-swipeable-views": "^0.14.1",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",

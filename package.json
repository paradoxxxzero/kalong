{
  "name": "kalong",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx --fix",
    "shell": "poetry run python -m kalong",
    "start": "concurrently --kill-others \"yarn dev\" \"wait-port 59998 && poetry run python -m kalong --front-port=59998 --detached $FILE\"",
    "pypublish": "yarn build && poetry publish --build"
  },
  "license": "GPL-3.0-or-later",
  "private": true,
  "dependencies": {
    "@codemirror/autocomplete": "^6.1.0",
    "@codemirror/lang-python": "^6.0.1",
    "@codemirror/legacy-modes": "^6.1.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@lezer/highlight": "^1.0.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.8",
    "@uiw/react-codemirror": "^4.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-swipeable-views": "^0.14.0",
    "redux": "^4.1.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^2.1.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "vite": "^3.1.0",
    "wait-port": "^1.0.0"
  }
}
